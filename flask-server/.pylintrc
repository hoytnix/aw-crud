[MESSAGES CONTROL]

disable=no-name-in-module,import-error,no-member,locally-disabled,fixme,too-few-public-methods,too-many-public-methods,invalid-name,global-statement,too-many-ancestors,missing-docstring

[FORMAT]

max-line-length=90

ignore-long-lines=^.*((https?:)|(pragma:)|(TODO:)).*$

[SIMILARITIES]

min-similarity-lines=4

ignore-imports=yes

[REPORTS]

reports=no

msg-template={msg_id} ({symbol}):{line:3d},{column}: {msg}
