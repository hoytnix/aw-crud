<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for url in urls %}
    <url>
        <loc>{{ config['CANONICAL_URL'] }}{{url|safe}}</loc>
        <lastmod>{{ last_modified(url).sitemap_date }}</lastmod>
    </url>
    {% endfor %}
</urlset>
